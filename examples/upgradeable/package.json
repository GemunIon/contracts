    "@openzeppelin/contracts-upgradeable": "4.7.3",
    "@openzeppelin/hardhat-upgrades": "1.21.0",
    "@openzeppelin/test-helpers": "0.5.16",
    "@typechain/ethers-v5": "10.1.0",
    "@typechain/hardhat": "6.1.3",
    "@types/chai": "4.3.3",
    "@types/mocha": "10.0.0",
    "@types/node": "16.18.2",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "chai": "4.3.6",
    "dotenv": "16.0.3",
    "eth-sig-util": "3.0.1",
    "ethereum-waffle": "4.0.7",
    "ethereumjs-util": "7.1.5",
    "ethers": "5.7.2",
    "hardhat": "2.12.1",
    "hardhat-deploy": "0.11.19",
    "hardhat-gas-reporter": "1.0.9",
    "rimraf": "3.0.2",
    "solhint": "3.3.7",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "0.8.2",
    "typechain": "8.1.0",
    "web3": "1.8.0"
  },
  "engines": {
    "node": ">=12.13"
  },
  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "name": "@gemunion/examples-upgradeable",
  "peerDependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "ethers": "^5.6.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gemunion/contracts"
  },
  "files": [
    "/sol/**/*.sol",
    "/abi/*.json"
  ],
  "scripts": {
    "build": "node_modules/.bin/hardhat compile",
    "clean": "node_modules/.bin/hardhat clean",
    "deploy:mainnet": "node_modules/.bin/hardhat --network mainnet deploy",
    "deploy:mumbai": "node_modules/.bin/hardhat --network mumbai deploy",
    "deploy:polygon": "node_modules/.bin/hardhat --network polygon deploy",
    "deploy:rinkeby": "node_modules/.bin/hardhat --network rinkeby deploy",
    "lint": "node_modules/.bin/eslint . --fix",
    "prebuild": "node_modules/.bin/hardhat clean",
    "preversion": "npm run build",
    "solhint": "node_modules/.bin/solhint -f table contracts/**/*.sol contracts/**/**/*.sol",
    "test": "node_modules/.bin/hardhat test",
    "test:coverage": "node_modules/.bin/hardhat coverage",
    "release:abi": "sh scripts/publish-abi.sh"
  },
  "types": "./typechain-types/index.ts",
  "version": "1.0.3"
}
